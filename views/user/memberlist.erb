      <div class = "container mt-5 pr-5 pl-5">
          <div class = "text-center">
          <h3>회원리스트 보기</h3>
          </div>
          <div class = "text-right my-3">
      </div>



 <table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Password</th>
      <th scope="col">Email</th>
      <th scope="col">삭제하기</th>
    </tr>
  </thead>
  
  <tbody>
    <% @user.each do |u| %>    
    <tr>
      <th scope="row"><%= u.id %></th>
      <td><%= u.username %></td>
      <td><%= u.password %></td>
      <td><%= u.email %></td>
      <td><a href = "/user/delete/<%= u.id %>">삭제</a></td>
    </tr>
    <% end %>
  </tbody>
  
</table>
        
</div>

